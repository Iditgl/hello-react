pipeline {
  environment {
    registry = "iditg/hello-world"
    registryCredential = 'dockerhub'
    dockerImage = ''
  }
  agent any
  stages {
    stage('Git Clone') {
      steps {
        git 'https://github.com/hashicorp/consul-helm.git'
      }
    }
    stage('Install Consul') {
      steps {
        sh "chmod +x consul.sh"
        sh "./consul.sh"
      }
    }

        }
      }
