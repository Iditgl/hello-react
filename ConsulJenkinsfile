pipeline {
  #environment {
  #}
  agent any
  stages {
    stage('Git Clone') {
      steps {
        git 'https://github.com/hashicorp/consul-helm.git'
      }
    }
    stage('Install Consul') {
      steps {
        sh "sudo cd consul-helm"
        sh "sudo git checkout v0.1.0"
        sh "sudo helm install --name consul ./"
     }
    }

        }
      }
